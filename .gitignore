node_modules/
vendor/
.npm/
.m2/

# Ignore generated files
coverage/
dist/
target/
tmp/

# Ignore log files
*.err
*.log
*.tmp

# Code editor files
.idea/
.vs/
.vscode/
.code-workspace/
*.sublime-project
